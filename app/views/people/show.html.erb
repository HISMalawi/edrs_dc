			function __$(id){
	        	return document.getElementById(id);
		    }

		    function resizeDiv(){
		      var wH = window.innerHeight; 
		      __$("containt").style.height = (wH *0.81 )+"px";
		      __$("table_container").style.height = (wH * 0.72) +"px";
		    }
	  
