<style type="text/css">
		input[type=radio].css-checkbox {
			position:absolute; z-index:-1000; left:-1000px; overflow: hidden; clip: rect(0 0 0 0); height:1px; width:1px; margin:-1px; padding:0; border:0;
		}

		input[type=radio].css-checkbox + label.css-label {
							padding-left:24px;
							height:19px; 
							display:inline-block;
							line-height:19px;
							background-repeat:no-repeat;
							background-position: 0 0;
							font-size:1.0em;
							vertical-align:middle;
							cursor:pointer;

		}

		input[type=radio].css-checkbox:checked + label.css-label {
							background-position: 0 -19px;
		}
		label.css-label {
				background-image:url(/assets/checkbox/checkbox1.png);
				-webkit-touch-callout: none;
				-webkit-user-select: none;
				-khtml-user-select: none;
				-moz-user-select: none;
				-ms-user-select: none;
				user-select: none;
		}
		#table_container{

				overflow-y: hidden;

				margin-bottom : 0.3em;

				box-shadow: 3.5px 3.5px 1.5px #888888;

				width: 86%;
				margin-left: 5%;

				border: 1px solid #bcb5b5;
				margin-top: 1%;


		}
		#summary{
			width:96% !important;
			margin: auto;
			margin-bottom: 1%;

		}
		#summary td {

				background: #ffffff;
				text-align: left !important;
				padding: 10px;
				padding-left: 10px;

		}
		.clickable{

			font-weight: bold;
			font-size:0.9em !important;
		}
		.clickable:hover{

				color: #00bce0;

				cursor: pointer;

		}
		#popup_details{
			width: 90%;
			margin: auto;
		}
		#popup_details th ,#popup_details td {

			text-align: left;
			font-size: 0.8em;
			padding: 0.5em;
		}
		#popup_details tr:nth-child(odd){
				background: #ffffff;
		}
		.orange{
			background: #ce731b;
			border:1px solid #ba6614;
		}

		.orange:hover{

			background: #d16b07;
		}

		.verification{
			width:96% !important;
			margin: auto;
			margin-bottom: 1%;
		}
		.verification td {

				background: #ffffff;
				text-align: left !important;
				padding: 10px;
				padding-left: 10px;
				width: 40%;
		}

</style>

<div style="position : absolute; width : 80%; bottom:4%;right :1%; margin-right : 1%">
  	<% if params[:next_url] %>
        <button class="green" style="float: right; margin-right:1%" onmousedown="unLock()"><span>Back</span></button>

  <% elsif @targeturl.present? %>

        <button class="green"  style="float: right; margin-right:1% ; width:100px" onmousedown="unLock()"><span>Back</span></button>

  <%else %>
         <button  style="float: right; margin-right:1%;width:100px" onmousedown="unLock()"><span>Back</span></button>
   <%end %>

  <% if User.current_user.district_code == @person.district_code %>

	<% if User.current_user.activities_by_level("#{@facility_type}").include?("Check for duplicates") %>

		<% if @status.status == "DC ACTIVE" || @status.status == "DC INCOMPLETE"  || @status.status =="DC REJECTED" %>

			<button style="float: right; margin-right:1%" onmousedown="checkCompleteness()"><span>Completeness Check</span></button>

		<%else%>

			<!--button class="gray" style="float: right; margin-right:1%"><span>Completeness Check</span></button-->

		<%end%>
		<%if ["DC ACTIVE"].include? @status.status && false%>
		
			<button style="float: right; margin-right:3%" onmousedown="markAsPending()"><span>Mark Pending</span></button>
		<%end%>

			
	<% end %>

	<% if User.current_user.activities_by_level("#{@facility_type}").include?("Approve a record") %>
		<%if SETTINGS['site_type'] == 'remote' && !SETTINGS['include_lo_role'] %>
			<%if  (@status.present? && @status.status == "DC ACTIVE") %>

					<button style="float: right; margin-right:3%" onmousedown="approveRecord()"><span>Approve</span></button>

			<%end%>
			<% if (@status.present? && @status.status == "HQ REJECTED") %>
				<button style="float: right; margin-right:3%" onmousedown="window.location='/people/form_container?url=/dc/add_reaprove_comment/<%= @person.id %>?next_url=<%=params[:next_url]%>?'"><span>Reapprove</span></button>
			<%end%>
		<%else%>
			<%if  (@status.present? && @status.status == "DC COMPLETE") %>
					<button style="float: right; margin-right:3%" onmousedown="approveRecord()"><span>Approve</span></button>

			<%end%>

			<% if (@status.present? && @status.status == "HQ REJECTED") %>
				<button style="float: right; margin-right:3%" onmousedown="window.location='/people/form_container?url=/dc/add_reaprove_comment/<%= @person.id %>?next_url=<%=params[:next_url]%>?'"><span>Reapprove</span></button>
			<%end%>
		<%end%>
	<%end%>

	<% if User.current_user.activities_by_level("#{@facility_type}").include?("Approve a record") %>
		<%if SETTINGS['site_type'] == 'remote' && !SETTINGS['include_lo_role']%>
			<%if  (@status.present? && @status.status == "DC ACTIVE")%>

					<button class="red" style="float: right; margin-right:3%" onmousedown="rejectRecord()"><span>Reject</span></button>

			<%end%>
		<%else%>
			<%if  @status.present? && @status.status == "DC COMPLETE" %>

				<button class="red" style="float: right; margin-right:3%" onmousedown="rejectRecord()"><span>Reject</span></button>

			<%end%>
		<%end%>
		<% if @person.status  == "DC LOST" || @person.status  == "DC DAMAGED"%>
			<button style="float: right; margin-right:1%" onmousedown="window.location='/dc/sent_to_hq_for_reprint/<%= @person.id%>'"><span>Send Record to HQ</span></button>
		<%end%>
	<%end%>

	<%if (@status.present? && @status.status == "HQ PRINTED") ||  (@status.present? && @status.status == "HQ CLOSED") || (@status.present? && @status.status == "HQ DISPATCHED") || (@status.present? && @status.status == "DC PRINTED")  || (@status.present? && @status.status == "DC DISPATCHED")%>
			
			<button  style="float: right; margin-right:3%" onmousedown="mark_for_reprint();"><span>Mark for Reprint</span></button>
	<%end%>
	<%if  (@status.present? && @status.status == "HQ PRINTED") || (@status.present? && @status.status == "HQ CLOSED") || (@status.present? && @status.status == "HQ DISPATCHED")  || (@status.present? && @status.status == "DC PRINTED")  || (@status.present? && @status.status == "DC DISPATCHED")%>
			<button class="orange" style="float: right; margin-right:3%" onmousedown="mark_for_amendment()"><span>Amend Record</span></button>

	<%end%>
	<%if (@status.present? && @status.status == "DC REPRINT") && false%>
			<button style="float: right; margin-right:3%" onmousedown="window.location = '/dc/approve_reprint/<%= @person.id %>?next_url=<%=params[:next_url]%>'"><span>Proceed to Re-Print</span></button>

	<%end%>

	<%if  @status.present? && ["HQ CAN PRINT","HQ CAN PRINT AMENDED","HQ CAN PRINT LOST","HQ CAN PRINT DAMAGED", "HQ CAN RE PRINT"].include?(@status.status) %>
			<button  id="print_btn" class="gray" style="float: right; margin-right:3%" ><span>Print Preview</span></button>

	<%end%>
 <%end%>
	<% if @comments.count > 0 %>
		<button style="float: right; margin-right:3%" onmousedown="viewPersonComment()"><span>View Record's Trail</span></button>
	<%end%>
	<% if SETTINGS['site_type'] == "dc"%>

		<% if @burial_report.blank? && User.current_user.role != "ADR"%>

			<button style="float: right; margin-right:3%" onmousedown="window.location = '/people/form_container?url=/dc/new_burial_report/<%= @person.id %>?next_url=<%=request.env['REQUEST_URI']%>'"><span>Add Burial Report</span></button>
		<%else  @burial_report.present? %>
		
			<button style="float: right; margin-right:3%" onmousedown="viewBurialReport()"><span>View Burial Report</span></button>

		<% end %>
	<%end%>

</div>

<div id="table_container">

